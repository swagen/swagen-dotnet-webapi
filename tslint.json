{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "interface-name": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": [
            true,
            "single"
        ]
    },
    "rulesDirectory": []
}
