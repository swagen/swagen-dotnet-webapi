{
    "name": "swagen-dotnet-webapi",
    "version": "1.0.0-beta.11",
    "description": "Swagen generator for creating empty ASP.NET Web API services",
    "main": "./lib/index.js",
    "typings": "./typings/index.d.ts",
    "scripts": {
        "watch": "tsc --watch",
        "build": "tsc",
        "lint": "tslint --project tsconfig.json --format verbose",
        "update": "npm un --save swagen codewriter lodash && npm i --save swagen codewriter lodash",
        "bump": "npm version prerelease --no-git-tag-version",
        "prepublishOnly": "npm run build && npm run lint",
        "postpublish": "npm run bump",
        "test": "npm run build && node ./test-harness/index"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/swagen/swagen-dotnet-webapi.git"
    },
    "keywords": [
        "swagen",
        "dotnet",
        "webapi"
    ],
    "author": "Swagen <swagen@jeevanjames.com>",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/swagen/swagen-dotnet-webapi/issues"
    },
    "homepage": "https://github.com/swagen/swagen-dotnet-webapi#readme",
    "dependencies": {
        "codewriter": "^1.3.0",
        "lodash": "^4.17.11",
        "swagen": "^1.0.0-beta.51"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.120",
        "@types/node": "^10.12.21",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1"
    }
}
